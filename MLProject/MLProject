name: StudentsPerformance_RF

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      input:
        type: str
      target:
        type: str
      test_size:
        type: float
        default: 0.2
      random_state:
        type: int
        default: 42
    command: >
      python modelling.py
      --input {input}
      --target {target}
      --test_size {test_size}
      --random_state {random_state}
